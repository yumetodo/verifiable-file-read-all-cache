{
  "name": "verifiable-file-read-all-cache",
  "version": "1.1.2",
  "description": "Read file all and cache it. Also, you can verify sha-256 hash.",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "scripts": {
    "test:ava": "ava --verbose",
    "test:ava:ci": "ava --verbose --tap | npx tap-xunit > results/ava.xml",
    "test:esm": "node test-esm.mjs",
    "test:cjs": "node test-cjs.js",
    "test": "run-p test:ava test:esm test:cjs",
    "test:ci": "run-p test:ava:ci test:esm test:cjs",
    "eslint": "eslint . --cache",
    "eslint:fix": "eslint --fix . --cache",
    "prettier": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yumetodo/verifiable-file-read-all-cache.git"
  },
  "keywords": [
    "cache",
    "fs",
    "file",
    "file cache"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "author": "yumetodo",
  "license": "BSL-1.0",
  "dependencies": {
    "await-mutex": "^1.0.2",
    "promise-fs": "^2.1.1",
    "sha.js": "^2.4.12"
  },
  "devDependencies": {
    "ava": "^4.3.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-node": "^11.1.0",
    "esm": "^3.2.25",
    "npm-run-all": "^4.1.5",
    "prettier": "3.6.2"
  },
  "ava": {
    "nodeArguments": [
      "--loader=./test/loader.mjs"
    ],
    "files": [
      "test/test.js"
    ]
  }
}
